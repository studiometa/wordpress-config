{#
/**
 * @file
 * Page Template Builder
 *
 * @param {array} $builder
 */
#}
{% extends 'layouts/base.twig' %}

{% block content %}
  <div class="container mx-auto">
    {% for item in builder %}
      {% set block_name = item.acf_fc_layout %}
      {% include [
        'organisms/blocks/' ~ block_name|replace({'_': '-'}) ~ '.twig',
        'organisms/blocks/default.twig'
      ] with {
        block: item,
        block_name: block_name
      } only %}
    {% endfor %}
  </div>
{% endblock %}
