{#
/**
 * @file
 * Block Wysiwyg
 * Display content from wysiwyg
 *
 * Available variables:
 * - block.content {Wysiwyg}
 */
#}

<section class="block-wysiwyg">
  <div class="container">
    <div class="grid grid-cols-12 gap-5">
      <div class="col-span-12">
        <div class="wysiwyg">
          {{ block.content|e('wp_kses_post')|raw }}
        </div>
      </div>
    </div>
  </div>
</section>
