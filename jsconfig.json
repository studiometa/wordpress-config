{
  "include": ["web/wp-content/themes/studiometa/src/js/**/*.js"],
  "compilerOptions": {
    "paths": {
      "#css/*": ["./web/wp-content/themes/studiometa/src/css/*"],
      "#fonts/*": ["./web/wp-content/themes/studiometa/src/fonts/*"],
      "#js/*": ["./web/wp-content/themes/studiometa/src/js/*"],
      "#svg/*": ["./web/wp-content/themes/studiometa/src/svg/*"],
      "#theme/*": ["web/wp-content/themes/studiometa/src/*"]
    },
    "target": "es2015",
    "module": "esnext",
    "noEmit": true,
    "checkJs": true,
    "strict": false,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "moduleResolution": "node"
  }
}
